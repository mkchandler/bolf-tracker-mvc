@model IEnumerable<BolfTracker.Models.Hole>
@{
  ViewBag.Title = "Holes - League Manager - Bolf Tracker";
}
<div class="page-header">
  <div class="row">
    <div class="col-md-8">
      <h1>League Manager</h1>
    </div>
  </div>
</div>
<ul class="nav nav-tabs">
  <li>@Html.ActionLink("Statistics", "Index", "Admin")</li>
  <li>@Html.ActionLink("Players", "Players", "Admin")</li>
  <li class="active">@Html.ActionLink("Holes", "Holes", "Admin")</li>
</ul>
<div class="row">
  <div class="col-md-6">
    <div class="table-responsive">
      <table class="table table-condensed table-bordered table-striped">
        <thead>
          <tr>
            <th>Number</th>
            <th>Par</th>
          </tr>
        </thead>
        <tbody>
          @foreach (var item in Model)
          {
            <tr>
              <td>@item.Id</td>
              <td>@item.Par</td>
            </tr>
          }
        </tbody>
      </table>
    </div>
  </div>
</div>
<p>
  @Html.ActionLink("Create Hole", "Create", "Hole", null, new { @class = "btn" })
</p>
