@model IEnumerable<BolfTracker.Models.PlayerStatistics>
<div class="table-responsive">
  <table class="table table-condensed table-bordered table-striped" id="monthly-stats">
    <thead>
      <tr>
        <th title="Month">Month</th>
        <th title="Year">Year</th>
        <th title="Shots Made">Made</th>
        <th title="Attempts">Attempts</th>
        <th title="Shooting Percentage">Shooting %</th>
        <th title="Points">P</th>
        <th title="Points Per Game">PPG</th>
        <th title="Wins">W</th>
        <th title="Losses">L</th>
        <th title="Winning Percentage">Win %</th>
        <th title="Pushes">Pushes</th>
        <th title="Pushes Per Game">Pushes PG</th>
        <th title="Steals">Steals</th>
        <th title="Sugar Free Steals">SFS</th>
      </tr>
    </thead>
    <tbody>
      @foreach (var monthlyStatistics in Model)
      {
        <tr>
          <td>@monthlyStatistics.Month</td>
          <td>@monthlyStatistics.Year</td>
          <td>@monthlyStatistics.ShotsMade</td>
          <td>@monthlyStatistics.Attempts</td>
          <td>@monthlyStatistics.ShootingPercentage</td>
          <td>@monthlyStatistics.Points</td>
          <td>@monthlyStatistics.PointsPerGame</td>
          <td>@monthlyStatistics.Wins</td>
          <td>@monthlyStatistics.Losses</td>
          <td>@monthlyStatistics.WinningPercentage</td>
          <td>@monthlyStatistics.Pushes</td>
          <td>@monthlyStatistics.PushesPerGame</td>
          <td>@monthlyStatistics.Steals</td>
          <td>@monthlyStatistics.SugarFreeSteals</td>
        </tr>
      }
    </tbody>
  </table>
</div>